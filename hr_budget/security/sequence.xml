<openerp>

<data noupdate="1">

	<record model="ir.sequence" id="hr_budget_seq">
		<field name="name">Budget Sequence</field>
		<field name="code">hr.budget</field>
		<field name="active">TRUE</field>
		<field name="prefix">HR-BUDGET-%(year)s-</field>
		<field name="padding">3</field>
		<field name="number_increment">1</field>
	</record>
	
	<record model="ir.sequence" id="hr_requisition_seq">
		<field name="name">Requisition Sequence</field>
		<field name="code">hr.requisition</field>
		<field name="active">TRUE</field>
		<field name="prefix">HR-REQU-%(year)s-</field>
		<field name="padding">3</field>
		<field name="number_increment">1</field>
	</record>

	<record model="ir.sequence" id="medical_insurance_seq">
		<field name="name">MI Sequence</field>
		<field name="code">medical.insurance</field>
		<field name="active">TRUE</field>
		<field name="prefix">MI/%(year)s/</field>
		<field name="padding">4</field>
		<field name="number_increment">1</field>
	</record>

	<record model="ir.sequence" id="medical_insurance_parents_seq">
		<field name="name">MIP Sequence</field>
		<field name="code">medical.insurance.parents</field>
		<field name="active">TRUE</field>
		<field name="prefix">MIP/%(year)s/</field>
		<field name="padding">4</field>
		<field name="number_increment">1</field>
	</record>


</data>

</openerp>